{
    "name": "ecentria/api-skeleton",
    "description": "Ecentria API Skeleton",
    "type": "project",
    "homepage": "http://stash.dev.opticplanet.net/projects/SVC/repos/ecentria-api-skeleton",
    "license": "proprietary",
    "authors": [
        {
            "name": "Sergey Chernecov",
            "email": "sergey.chernecov@ecentria.com"
        }
    ],
    "repositories": [
        { "type": "vcs", "url": "ssh://git@stash.dev.opticplanet.net/lib/ecentria-logging-library.git" },
        { "type": "vcs", "url": "ssh://git@stash.dev.opticplanet.net/lib/ecentria-logging-bundle.git" }
    ],
    "require": {
        "php": "^7.3.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/api-pack": "^1.2",
        "doctrine/doctrine-migrations-bundle": "^2.1",
        "symfony/console": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^4.4",
        "symfony/monolog-bundle": "^3.5",
        "symfony/translation": "4.4.*",
        "symfony/yaml": "^4.4",
        "ecentria/logging-bundle": "^0.4.3"
    },
    "require-dev": {
        "deployer/deployer": "^6.7",
        "laminas/laminas-code": "^3.4",
        "phpunit/phpunit": "^9.0",
        "symfony/maker-bundle": "^1.14",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/process": "4.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
